{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 13
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "semi": ["warn", "never"],
    "indent": ["error", 2], // 이거 오류나면 지워주세요
    "quotes": ["warn", "single"]
  }
}
